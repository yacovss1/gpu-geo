  "description": "GPU-accelerated geographic rendering engine using WebGPU compute shaders for coordinate transformation and future cloth-physics triangulation",
  "main": "dist/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YOUR-USERNAME/gpu-geo.git"
  },
  "bugs": {
    "url": "https://github.com/YOUR-USERNAME/gpu-geo/issues"
  },
  "homepage": "https://github.com/YOUR-USERNAME/gpu-geo#readme",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
