{
  "name": "map-active-work-typescript",
  "version": "1.0.0",
  "description": "TypeScript implementation of the advanced WebGPU mapping system with hidden buffer technology",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "clean": "rimraf dist",
    "start": "npm run dev"
  },
  "keywords": [
    "webgpu",
    "mapping",
    "gis",
    "coordinates",
    "hidden-buffer",
    "typescript",
    "translation-layer"
  ],
  "author": "JS",
  "license": "MIT",
  "dependencies": {
    "@webgpu/types": "^0.1.38",
    "gl-matrix": "^3.4.3",
    "earcut": "^2.2.4",
    "pbf": "^4.0.1",
    "@mapbox/vector-tile": "^2.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/earcut": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@eslint/js": "^9.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/map-active-work"
  },
  "bugs": {
    "url": "https://github.com/your-org/map-active-work/issues"
  },
  "homepage": "https://github.com/your-org/map-active-work#readme"
}